{
  "name": "filebrowser-frontend",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "run-s clean build:production",
    "lint": "npx vue-cli-service lint --no-fix",
    "build:dev": "vue-cli-service build --mode development --no-clean",
    "build:production": "vue-cli-service build --no-clean",
    "clean": "del-cli dist/**/!(\\.gitignore)",
    "fix": "npx vue-cli-service lint",
    "watch": "run-s clean watch:*",
    "watch:vue": "vue-cli-service build --mode development --watch --no-clean"
  },
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "ace-builds": "^1.4.12",
    "bootstrap": "^4.5.2",
    "bootstrap-vue": "^2.17.3",
    "clipboard": "^2.0.8",
    "core-js": "^3.15.2",
    "css-vars-ponyfill": "^2.4.5",
    "filesize": "^7.0.0",
    "js-base64": "^3.6.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "material-design-icons": "^3.0.1",
    "moment": "^2.29.1",
    "mutationobserver-shim": "^0.3.7",
    "normalize.css": "^8.0.1",
    "noty": "^3.2.0-beta",
    "popper.js": "^1.16.1",
    "portal-vue": "^2.1.7",
    "pretty-bytes": "^5.6.0",
    "qrcode.vue": "^1.7.0",
    "utif": "^3.1.0",
    "vue": "^2.6.14",
    "vue-i18n": "^8.25.0",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.5.2",
    "vuex": "^3.6.2",
    "vuex-router-sync": "^5.0.0",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0-beta.2",
    "@vue/cli-plugin-eslint": "~5.0.0-beta.2",
    "@vue/cli-service": "~5.0.0-beta.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "compression-webpack-plugin": "^8.0.1",
    "del-cli": "^4.0.1",
    "eslint": "^7.31.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.14.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "sass": "^1.35.2",
    "sass-loader": "^12.1.0",
    "vue-cli-plugin-bootstrap-vue": "~0.8.0",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "rules": {
      "vue/no-mutating-props": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
